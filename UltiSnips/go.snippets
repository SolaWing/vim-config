
snippet v "var declare" b
var $1 ${2:string}
endsnippet

snippet l "const declare" b
const $1 ${2:string}
endsnippet

snippet f "func" w
func $1($2) {
	$0
}
endsnippet

snippet m "method declare" b
func (self ${1:type}) ${2:method}($3)  {
	$0
}
endsnippet

snippet switch_type "switch var to type" b
switch ${2:v} := ${1:i}.(type) {
case ${3:T}:
default:
}
endsnippet
