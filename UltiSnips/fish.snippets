priority 1

snippet if "if statement" b
if ${1:test -e $2}
	${VISUAL}$0
end
endsnippet

snippet eif "else if statement" b
else if ${1:test -e $2}
	${VISUAL}$0
endsnippet

snippet begin "begin block"
begin $0; end
endsnippet

snippet for "for block" b
for ${1:i} in $2
	$0
end
endsnippet

snippet contains "contains"
contains ${1:key} ${2:Values}
endsnippet

snippet alias "alias functions" b
function $1 --wraps '$2'
	$2 $argv
end
endsnippet
